{
    "name": "lingoda/thinkingcap-client",
    "description": "Lingoda Thinkingcap Bundle",
    "type": "symfony-bundle",
    "require-dev": {
        "laminas/laminas-code": "^4.5",
        "phpspec/phpspec": "^7.1.x-dev",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.3"
    },
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "phpro/soap-client": "^1.7",
        "symfony/framework-bundle": "^4.4 || ^5.2",
        "symfony/yaml": "^4.4 || ^5.2"
    },
    "conflict": {
        "psr/cache": ">=2.0.0",
        "symfony/cache": ">=6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Lingoda\\ThinkingcapBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lingoda\\ThinkingcapBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
